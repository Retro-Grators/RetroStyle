{

	"manifest_version": 2,
  
	"name": "Retro Grator",
  
	"description": "Change style of webpages",
  
	"version": "1.0",
  
	"browser_action": {
		"default_icon": {
			"16": "icon16.png",
			"32": "icon32.png",
			"64": "icon64.png"
		},
		"default_popup": "popup.html"
	},
  
	"permissions": [
		"activeTab",
		"tabs", "http://*/*", "https://*/*"
	]
  
  }