{

	"manifest_version": 2,
  
	"name": "Retro Style",
  
	"description": "Change style of webpages",
  
	"version": "1.0",
  
	"browser_action": {
		"default_icon": {"64": "icon64.png"},
		"default_popup": "popup.html"
	},
  
	"permissions": [
		"activeTab",
		"tabs", "http://*/*", "https://*/*"
	]
  
  }